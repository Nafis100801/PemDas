#include <iostream>

using namespace std;
int main()
{
int nilai;
    cout << "Masukan Nilai : ";
    cin >> nilai;
    cout << "\n";
    switch(nilai){
        case 1:
            cout << "nilai yang anda masukan 1" << endl;
            break;
        case 2:
            cout << "nilai yang anda masukan 2" << endl;
            break;
        case 3:
            cout << "nilai yang anda masukan 3" << endl;
            break;
        default:
            cout << "no" << endl;
            break;
    }
 }
